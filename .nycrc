{
  "include": [
    "server/**/0*.js",
    "game/**/*.js"
  ],
  "exclude": [
    "test/**/*.js"
  ],
  "extension": [
    ".js"
  ],
  "reporter": [
    "text-summary",
    "lcovonly"
